   -
     name : RafraîchitEtat
     procedure_id : 1200827148920431562
     type_code : 14
     code : |1-
      
      // Résumé : Regarde si les notifications sont activées
      //
      PROCEDURE RafraîchitEtat()
      
      si ChargeParamètre(gsPréférenceNotificationsActivées,faux) ALORS
      	gbActif = vrai
      	BTN_Valider = "Désactiver les notifications"
      	SAI_Périodocité..Visible = faux
      SINON
      	gbActif = faux
      	BTN_Valider = "Activer les notifications"
      	SAI_Périodocité..Visible = vrai
      FIN
     type : 458752
   -
     name : ActiveApplications
     procedure_id : 1200830095268092169
     type_code : 14
     code : |1-
      // Résumé : Activation des notifications
      // Syntaxe :
      //ActiveApplications (<bActiver> est booléen)
      //
      // Paramètres :
      //	bActiver (booléen) : <indiquez ici le rôle de GèreNotifications>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ActiveApplications(bActiver est un booléen)
      
      Si bActiver ALORS
      	// Activation des notifications
      	gbActif = Vrai
      	
      	SauveParamètre(gsPréférenceNotificationsActivées,Vrai)
      	
      	SI ThreadEtat(gsNomThreadPersistant) = threadInexistant ALORS
      		ThreadExécute(gsNomThreadPersistant,threadContexteGlobal,GèreNotifications)
      		ThreadPersistant(gsNomThreadPersistant,gNotificationPersistante)
      	SINON
      		// Les notifications sont déjà actives
      	FIN
      SINON
      	// Désactivation des notifications
      	gbActif = faux
      	SauveParamètre(gsPréférenceNotificationsActivées,faux)
      	NotifSupprime(gNotificationPonctuelle)
      FIN
      
      // Rafraîchit l'état
      RafraîchitEtat()
     type : 458752
